{
  "name": "brainbot",
  "version": "1.0.0",
  "description": "brainbot",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "brainbot",
    "discord",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/EveripediaNetwork/brainbot/issues"
  },
  "repository": {
