    "url": "git@github.com:EveripediaNetwork/brainbot.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
