    "discordx": "^9.5.3",
    "graphql-request": "^4.2.0",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.0.7",
    "@types/node": "^17.0.31",
    "@types/node-schedule": "^2.1.0",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
